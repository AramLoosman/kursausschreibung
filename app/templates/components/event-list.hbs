{{#event-list-search queryChanged=(action "queryChanged") events=events as |filteredEvents|}}
  {{#list-pagination page=page items=filteredEvents as |eventsOnCurrentPage|}}
    {{#each eventsOnCurrentPage as |event|}}
      {{event-list-item event=event}}
    {{/each}}
  {{/list-pagination}}
{{/event-list-search}}